  <lyric>
    <block type="verse">
      <row important_over="true"><ch a="e"/> Nic nie mam</row>
      <row important_over="true"><ch a="F"/> Zdmuchnęła mnie ta jesień c<ch a="e"/>ałkiem</row>
      <row important_over="true">N<ch a="e"/>awet nie wiem</row>
      <row important_over="true"><ch a="F"/> Jak tam sprawy za l<ch a="e"/>asem</row>
      <row important_over="true">R<ch a="G"/>ano wst<ch a="Fis"/>aję po<ch a="h"/>emat chwalę</row>
      <row important_over="true">Bi<ch a="C"/>orę się za sł<ch a="D"/>owo jak za chl<ch a="e"/>eb</row>
    </block>
    <block type="chorus">
      <bis>
        <row important_over="true">Rzeczyw<ch a="e"/>iście t<ch a="D"/>ak jak ksi<ch a="G"/>ężyc</row>
        <row important_over="true">Ludzie zn<ch a="a"/>ają mnie tylko z j<ch a="e"/>ednej</row>
        <row important_over="true"><ch a="D"/> Jesiennej str<ch a="a"/>on<ch a="e"/>y</row>
      </bis>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="e"/> Nic nie mam</row>
      <row important_over="false"><ch a="F"/> Tylko z daszkiem nieba zamyślony k<ch a="e"/>aszkiet</row>
      <row important_over="false">Ni<ch a="e"/>e zważam</row>
      <row important_over="false"><ch a="F"/> Na mody byle j<ch a="e"/>akie</row>
      <row important_over="false">P<ch a="G"/>iszę wył<ch a="Fis"/>ącznie p<ch a="h"/>iszę wyłącznie</row>
      <row important_over="false"><ch a="C"/>Uczuć st<ch a="D"/>arym drap<ch a="e"/>akiem</row>
    </block>
    <block type="chorus">
      <bis times="3">
        <row important_over="false">Rzeczyw<ch a="e"/>iście t<ch a="D"/>ak jak ksi<ch a="G"/>ężyc</row>
        <row important_over="false">Ludzie zn	<ch a="a"/>ają mnie tylko z j<ch a="e"/>ednej</row>
        <row important_over="false"><ch a="D"/> Jesiennej str<ch a="a"/>on<ch a="e"/>y</row>
      </bis>
    </block>
