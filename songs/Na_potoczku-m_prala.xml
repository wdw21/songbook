	<lyric xmlns="http://21wdh.staszic.waw.pl">
		<block type="verse">
			<row important_over="false">Na potoczku-m prała,<ch a="d"/></row>
			<row important_over="false">Na werboczku-m kłała,<ch a="A7"/></row>
			<row important_over="false">Pomoży mi, Pan’e Boże,<ch a="d"/></row>
			<row important_over="false">Szczo-m sobi dumała.<ch a="A7"/><ch a="d"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Jam sobi dumała<ch a="d"/></row>
			<row important_over="false">Chłopcia mołodoho,<ch a="A7"/></row>
			<row important_over="false">A mene wydali<ch a="d"/></row>
			<row important_over="false">Za dida staroho.<ch a="A7"/><ch a="d"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Jak mn’e wydawali,<ch a="d"/></row>
			<row important_over="false">Tak mene kazali,<ch a="A7"/></row>
			<row important_over="false">Abyś moja Doniu<ch a="d"/></row>
			<row important_over="false">Dida czestuwała.<ch a="A7"/><ch a="d"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Ja dida czesuju,<ch a="d"/></row>
			<row important_over="false">Czestuwaty muszu,<ch a="A7"/></row>
			<row important_over="false">Pomoży mi, Pan’e Boże,<ch a="d"/></row>
			<row important_over="false">Wiźmi z dida duszu!<ch a="A7"/><ch a="d"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Wiźmi z dida duszu,<ch a="d"/></row>
			<row important_over="false">Taj powiś na chruszu,<ch a="A7"/></row>
			<row important_over="false">Szczuby starym znaty,<ch a="d"/></row>
			<row important_over="false">Jak mołodu braty.<ch a="A7"/><ch a="d"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Zabrał Boh staroho,<ch a="d"/></row>
			<row important_over="false">Nie dał mołodoho.<ch a="A7"/></row>
			<row important_over="false">Pomoży mi, Pan’e Boże,<ch a="d"/></row>
			<row important_over="false">Widdaj choć staroho!<ch a="A7"/><ch a="d"/></row>
		</block>
	</lyric>
	
	
