	<lyric xmlns="http://21wdh.staszic.waw.pl">
		<block type="verse">
			<row important_over="false">Cichutko, pomaleńku <ch a="e"/><ch a="D"/></row>
			<row important_over="false">Gdzie wiatr w gałęziach drzew <ch a="e"/><ch a="D"/></row>
			<row important_over="false">Dogania ciszę ptasich gniazd <ch a="C"/><ch a="G"/><ch a="D"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Gdzie gwiazd milczących dal<ch a="e"/><ch a="D"/></row>
			<row important_over="false">Odległa, niewiadoma<ch a="e"/><ch a="D"/></row>
			<row important_over="false">Ukryta w bezmiar swej piękności<ch a="C"/><ch a="G"/><ch a="D"/></row>
		</block>
		<block type="verse">
			<row important_over="false">Cichutko, pomaleńku<ch a="e"/><ch a="D"/></row>
			<row important_over="false">Przez świata dziwny świat<ch a="e"/><ch a="D"/></row>
			<row important_over="false">Nie zważaj na okrutne oczy dnia<ch a="C"/><ch a="G"/><ch a="D"/></row>
		</block>
	</lyric>
