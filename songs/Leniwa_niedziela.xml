      <row important_over="false">Nie ma t<ch a="C"/>akich co się spi<ch a="d"/>eszą,</row>
      <row important_over="false">Mają dz<ch a="a"/>iś obywat<ch a="E"/>ele</row>
      <row important_over="false">Len<ch a="E7"/>iwą niedzi<ch a="a"/>elę</row>
      <row important_over="false">
        <ch a="d"/>
        <ch a="a"/>
        <ch a="E"/>
        <ch a="E7"/>
     </row>
