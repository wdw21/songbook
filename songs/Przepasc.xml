  <lyric>
    <block type="verse">
      <row important_over="false"><ch a="e"/>Tego dnia obcy ludzie</row>
      <row important_over="false"><ch a="C"/>Zadławili we mnie tak zwaną<ch a="G"/><ch a="a"/></row>
      <row important_over="false"><ch a="C"/>Jasność spojrzenia<ch a="G"/></row>
      <row important_over="false"><ch a="e"/>W kanion strachu zagonili</row>
      <row important_over="false"><ch a="C"/>A jemu serce pękło<ch a="G"/><ch a="a"/></row>
      <row important_over="false"><ch a="G"/>W tej samej chwili</row>
      <row important_over="false"><ch a="C"/>A bez jego mocnej dłoni<ch a="G"/></row>
      <row important_over="false"><ch a="C"/>A bez tych pogodnych spojrzeń<ch a="G"/></row>
      <row important_over="false"><ch a="C"/>Czeka mnie<ch a="G"/></row>
      <row important_over="false"><ch a="D"/>Czeka mnie</row>
      <bis>
        <row important_over="false"><ch a="C"/>Przepaść<ch a="D"/><ch a="e"/></row>
        <row important_over="false"><ch a="C"/>Studnia dźwięków i mgła<ch a="D"/><ch a="e"/></row>
        <row important_over="false"><ch a="C"/>Przepaść<ch a="D"/><ch a="e"/></row>
        <row important_over="false"><ch a="C"/>Idę do niej tam<ch a="D"/><ch a="e"/></row>
      </bis>
