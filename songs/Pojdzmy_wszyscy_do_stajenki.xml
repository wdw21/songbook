<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl Song.xsd" title="Pójdźmy wszyscy do stajenki">
  <alias/>
  <text_author/>
  <comment/>
  <composer type="solo"/>
  <artist type="solo"/>
  <translator/>
  <original_title/>
  <lyric>
    <block type="verse">
      <row important_over="false">Pójdźmy wszyscy do stajenki</row>
      <row important_over="false">Do Jezusa i panienki</row>
      <bis>
        <row important_over="false"><ch a="d"/>Powitajmy maleńkiego<ch a="G"/><ch a="C"/><ch a="a"/></row>
        <row important_over="false"><ch a="G"/>I Maryję matkę jego<ch a="G7"/><ch a="C"/><ch a="a"/><ch a="C"/></row>
      </bis>
    </block>
    <block type="verse">
      <row important_over="false">Witaj Jezu ukochany</row>
      <row important_over="false">Od patriarchów czekany</row>
      <bis>
        <row important_over="false"><ch a="d"/>Od proroków ogłoszony<ch a="G"/><ch a="C"/><ch a="a"/></row>
        <row important_over="false"><ch a="G"/>Od narodów upragniony<ch a="G7"/><ch a="C"/><ch a="a"/><ch a="C"/></row>
      </bis>
    </block>
    <block type="verse">
      <row important_over="false">Witaj dzieciąteczko w żłobie</row>
      <row important_over="false">Wyznajemy Boga w tobie</row>
      <bis>
        <row important_over="false"><ch a="d"/>Coś się narodził tej nocy<ch a="G"/><ch a="C"/><ch a="a"/></row>
        <row important_over="false"><ch a="G"/>Byś nas wyrwał z czarta mocy<ch a="G7"/><ch a="C"/><ch a="a"/><ch a="C"/></row>
      </bis>
    </block>
  </lyric>
  <music metre="">
    <guitar barre="0"/>
  </music>
