<song title="Leonardo" xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl Song.xsd">

	<alias>W górę szlaban</alias>

	<text_author type="solo"></text_author>

	<comment>Specyficzne bicie.</comment>

	<composer type="solo"></composer>

	<artist type="solo"></artist>

	<translator></translator>

	<original_title></original_title>

	<lyric xmlns="http://21wdh.staszic.waw.pl">

		<block type="verse">

			<row important_over="true"><ch a="D"/> Ja nie wesoła, <ch a="A"/>ale z kokardą,</row>

			<row important_over="true">L<ch a="h"/>ecę do słońca, h<ch a="G"/>ej Leon<ch a="A"/>ardo!</row>

			<row important_over="false"><ch a="D"/> A ja się kręcę, b<ch a="A"/>o stać nie warto,</row>

			<row important_over="false">N<ch a="h"/>aprzód planeto, h<ch a="G"/>ej Leon<ch a="A"/>ardo!</row>

		</block>

		<block type="chorus">

			<bis times="2">

				<row important_over="false">D<ch a="D"/>ość jest wszystkiego, d<ch a="A"/>ojść można wszędzie.</row>

				<row important_over="false">D<ch a="h"/>ość jest wszystkiego, d<ch a="G"/>ojść można wsz<ch a="A"/>ędzie.</row>

			</bis>

		</block>

		<block type="verse">

			<row important_over="false"><ch a="D"/> Diabeł mnie trzyma, t<ch a="A"/>arga za włosy,</row>

			<row important_over="false">D<ch a="h"/>okąd, wariatko, chc<ch a="G"/>esz dziś wyr<ch a="A"/>uszyć?</row>

			<row important_over="false"><ch a="D"/> A ja gotowa, j<ch a="A"/>a z halabardą</row>

			<row important_over="false">L<ch a="h"/>ecę do słońca, h<ch a="G"/>ej Leon<ch a="A"/>ardo!</row>

		</block>

		<blocklink blocknb="2"/>

		<block type="verse">

			<row important_over="false"><ch a="D"/> Panie w koronie, P<ch a="A"/>anie z liczydłem,</row>

			<row important_over="false">Ni<ch a="h"/>e chcę być mrówką -- j<ch a="G"/>a chcę być szcz<ch a="A"/>ygłem,</row>

			<row important_over="false"><ch a="D"/> A moja sława, dr<ch a="A"/>oga i muzyka</row>

			<row important_over="false">D<ch a="h"/>o brązowego ż<ch a="G"/>ycia pom<ch a="A"/>yka.</row>

		</block>

		<blocklink blocknb="2"/>

		<block type="verse">

			<row important_over="false"><ch a="D"/> Przyszłam z bylekąd, <ch a="A"/>ale co z tego</row>

			<row important_over="false">Zmi<ch a="h"/>erzch jest daleko, h<ch a="G"/>ej, hej kol<ch a="A"/>ego!</row>

			<row important_over="false"><ch a="D"/> Odłóżmy sprawę, k<ch a="A"/>ochany synku,</row>

			<row important_over="false">N<ch a="h"/>a jakieś dziesięć mi<ch a="G"/>ejsc po przec<ch a="A"/>inku...</row>

		</block>

		<blocklink blocknb="2"/>

	</lyric>

